// Note, all int types are signed 64 bit

block fib () {
    
    // create some variables to store state
    int 1 -> x;
    int x -> oldx;
    int x -> olderx;

    // perform the sequence a number of times
    repeat 50 times {

        pr x;
        pr " ";

        int oldx -> olderx;
        int x -> oldx;

        // compute new x value
        int (olderx + oldx) -> x;
    }

    prn " ";

}

start {
    
    prn "Calling block fib";
    call fib ();

}