// Tests the parsing of a returnable function

returnable integer func (a, somelist) -> returned {
	echo_nl "Hello";
    echo_nl somelist;
	integer 5 + a -> returned;
	return returned;
}

start {
    list [some list] -> somelist;
	call func (1, somelist) -> result;
	echo "called: ";
	echo_nl result;
}
